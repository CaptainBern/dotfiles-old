command=$HOME/scripts/$BLOCK_NAME
separator_block_width=20
markup=pango

[volume]
command=$HOME/scripts/volume i3blocks
interval=once
signal=1

[brightness]
label=  
command=$HOME/scripts/brightness
interval=10
signal=2

[battery]
command=$HOME/scripts/battery -c
interval=30

